{
    "manifest_version": 3,
    "name": "Jetpens Comp",
    "version": "0.1",
  
    "description": "Companion extension for shopping at Jetpens",
  
    "content_scripts": [
      {
        "matches": ["*://*.jetpens.com/*"],
        "js": [ "dist/popper.js",
          "dist/tippy.js",
          "dist/siema.js",
          "js/ProductSlideShow.js",
          "js/ProductTooltip.js",
          "js/Template.js",
          "jetty.js" ],
        "css": ["jetty.css"]
      }
    ],

    "web_accessible_resources": [
      {
        "resources": ["templates/*.html"],
        "matches": ["*://*.jetpens.com/*"]
      }
    ]
  }
  